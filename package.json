{
  "name": "tm1ts",
  "version": "1.0.0",
  "description": "A TypeScript library that wraps the TM1 REST API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "tsc && node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "clean": "del-cli ./dist/*",
    "build": "npm run clean && tsc"
  },
  "keywords": [
    "TM1",
    "OData",
    "REST",
    "TypeScript",
    "API"
  ],
  "author": "George Bryant",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^14.6.4",
    "axios": "^0.21.1",
    "axios-cookiejar-support": "^1.0.0",
    "https": "^1.0.0",
    "tough-cookie": "^4.0.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "tsc": "^1.20150623.0"
  },
  "devDependencies": {
    "@types/tough-cookie": "^4.0.0",
    "@types/uuid": "^8.3.0",
    "del-cli": "^3.0.1",
    "nodemon": "^2.0.4",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2"
  }
}
